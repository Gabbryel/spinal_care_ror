<div class="navbar navbar-expand-sm navbar-light navbar-lewagon" id="navbar" data-action="scroll@window->toggleAdminPanel#navbarFixed">
  <div class="navbar-content">
    <%= link_to root_path, class: "navbar-brand" do %>
      <%= image_tag "logo2-color.png", class: 'navbar-brand-img', data: {sidemenu_toggle_target: 'toggle'}, alt: 'logo spinal care' %>
    <% end %>
    <div class="navbar-menu" id="navbar-menu">
        <ul class="team" id="team">
          <li class="<%= path_helper == '' ? 'active-page' : 'active-menu-item' %>"><%= link_to 'Acasă', root_path %><div></div></li>
          <li class="<%= path_helper == 'echipa' ? 'active-page' : 'active-menu-item' %>"><%= link_to 'Echipa medicală', echipa_path %><div></div></li>
          <li class="<%= path_helper == 'specialitati-medicale' ? 'active-page' : 'active-menu-item' %>"><%= link_to 'Specialități medicale', specialitati_medicale_path %><div></div></li>
          <li class="<%= path_helper == 'servicii-medicale' ? 'active-page' : 'active-menu-item' %>"><%= link_to 'Servicii medicale', servicii_medicale_path %><div></div></li>
          <li class="<%= path_helper == 'info-pacient-index' ? 'active-page' : 'active-menu-item' %>"><%= link_to 'Info pacient', info_pacient_index_path %><div></div></li>
          <% if current_user %>
            <li class="active-menu-item" id="nav-bar-exit-link">
              <%= link_to 'Ieșire', destroy_user_session_path, data: {turbo_method: :delete} %>
              <div></div>
            </li>
          <% end %>
        </ul>
    </div>
      <%= render partial: "shared/navbar/hamburger" %>
  </div>
</div>
