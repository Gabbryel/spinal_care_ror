


<div class="member-card"
  data-identity="<%= full_name(m) %>" data-profession="<%= m.profession.name %>" data-specialty="<%= m.specialty.name if m.specialty %>">
  <% if m.has_own_page %>
    <a href="../echipa/<%= m.slug %>" id="<%= m.slug %><%= path_for_id %>" name="<%= m.slug %><%= path_for_id %>" >
      <%= render 'members/member_card_info', m: m %>
    </a>
  <% else %>
    <%= render 'members/member_card_info', m: m %>
  <% end %>
</div>
