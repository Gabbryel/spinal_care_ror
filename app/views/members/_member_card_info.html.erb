<div class="member-card__main" title="<%= m.academic_title %> <%= full_name(m) %>">
  <div class="member-card__photo-holder" style="background-image: url('<%= cl_image_path(m.photo.key, :quality=>60, :fetch_format=>:webp, :width=>100) if m.photo.key.present? %>');">
  </div>
  <div class="member-card-info">
    <h1 class="member-card-info__name">
      <span><%= m.academic_title if m.academic_title != '-' %></span> <%= full_name(m) %>
    </h1>
    <div class="member-card-info__profession">
      <object>
        <%= link_to "../specialitati-medicale/#{m.specialty.slug unless m.specialty.nil? }", aria_label: "#{full_name(m)}" do %>
          <p class="member-card__specialty-name"><%= m.profession.name %> <%= m.doctor_grade %> <%= m.specialty.name unless m.specialty.nil? %></p>
        <% end %>
      </object>
    </div>
  </div>
</div>