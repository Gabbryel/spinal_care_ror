<%= turbo_frame_tag specialty do %>
<div class="specialty">
  <%= specialty.name %>
  <div class="specialty-crud">
    <%= link_to specialty_path(specialty), data: {
      turbo_frame: "_top" } do %>
      <i class="fas fa-eye"></i>
    <% end %>
    
    <%= link_to specialty_path(specialty), data: { turbo_method: :delete, turbo_confirm: "Sigur ștergi specialitatea mediclă #{specialty.slug}?"} do %>
      <i class="fas fa-trash-alt"></i>
    <% end %>
  </div>
</div>
<% end %>